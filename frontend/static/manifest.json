{
  "manifest_version": 3,
  "name": "Aegis",
  "version": "1.0",
  "key":"",
  "permissions": [
    "storage", 
    "activeTab", 
    "webNavigation", 
    "tabs"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*" 
  ],

  "action": {
    "default_popup": "chrome-extension/index.html"
  },
  "options_page": "settings/index.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "authentication/index.html",
        "authentication/assets/*",
        "*.js", "*.wasm", "*.json"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}